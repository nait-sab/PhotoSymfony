{% set slot = 0 %}
{% for element in liste %}
    {% if element != '.' and element != '..' %}
        <a href="{{ path('display_image', {id: ID[slot]}) }}">
            <img class="grilleImage" src="{{ path('afficher_image', {nom: element}) }}"/>
        </a>
    {% endif %}
    {% set slot = slot + 1 %}
{% endfor %}
